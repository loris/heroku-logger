{
  "name": "heroku-logger",
  "version": "0.3.0",
  "description": "A dead simple logger, designed to be perfect for Heroku apps.",
  "repository": "git://github.com/ianstormtaylor/heroku-logger.git",
  "main": "./lib/index.js",
  "dependencies": {
    "chalk": "^1.1.3",
    "figures": "2.0.0",
    "flat": "^2.0.1",
    "is": "^3.2.1",
    "logfmt": "^1.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
